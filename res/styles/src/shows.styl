.pshows
	background-color #eee
	
	&__timeline
		padding-top 10px
		margin 0 auto
		width 50%
		max-width 1000px
		
.pshow
	font-family "Iowan Old Style"
	display inline-block
	width 100%
	text-decoration none
	margin 20px 0px
	
	&__container
		display flex
		
	&__left
		background-color #adcfd3
		
	&__right
		flex-grow 1
		background-color #13535e
